#:kivy 1.0.9

<Cell>:
    width: self.skatgame.width / self.skatgame.cols
    height: self.skatgame.height / self.skatgame.rows
    x: self.col * self.width
    y: self.row * self.height
    canvas:
        Color:
            rgb: self.color, self.color, self.color
        Rectangle:
            pos:self.pos
            size:self.size

